    "dependencies": {
        "bcrypt": "5.0.0",
        "body-parser": "^1.18.3",
        "chai-http": "^4.3.0",
        "cloudinary": "^1.14.0",
        "cookie-parser": "^1.4.5",
        "cors": "^2.8.5",
        "datauri": "^2.0.0",
        "dotenv": "8.2.0",
        "express": "^4.16.4",
        "jsonwebtoken": "^8.5.1",
        "mongoose": "^5.4.17",
        "multer": "^1.4.1",
